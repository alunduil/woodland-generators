{
  "name": "woodland-generators",
  "version": "0.1.0",
  "description": "CLI tool for generating resources for Root: The Tabletop RPG",
  "main": "dist/index.js",
  "bin": {
    "woodland-gen": "dist/cli.js"
  },
  "author": "Alex Brandt",
  "license": "MIT",
  "keywords": [
    "root",
    "rpg",
    "tabletop",
    "generator",
    "cli",
    "character-generator",
    "npc-generator",
    "clearing-generator",
    "woodland",
    "leder-games",
    "pdf",
    "random-generator"
  ],
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "dependencies": {
    "commander": "^11.0.0"
  },
  "devDependencies": {
    "@types/node": "^24.1.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "depcheck": "^1.4.7",
    "eslint": "^8.56.0",
    "npm-check-updates": "^16.0.0",
    "prettier": "^3.0.0",
    "typescript": "^5.4.0"
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "cli": "npm run build && node dist/cli.js",
    "clean": "rm -rf dist/",
    "prepare": "npm run build",
    "prepublishOnly": "npm run check:all",
    "prepack": "npm run clean && npm run build",
    "postversion": "git push && git push --tags",
    "check:all": "npm run check:types && npm run check:lint && npm run check:format && npm run check:deps",
    "check:types": "tsc --noEmit",
    "check:lint": "eslint src/**/*.ts",
    "check:format": "prettier --check src/**/*.ts",
    "check:deps": "depcheck",
    "check:audit": "npm audit",
    "check:outdated": "npm outdated",
    "fix:lint": "eslint src/**/*.ts --fix",
    "fix:format": "prettier --write src/**/*.ts",
    "update:deps": "ncu"
  }
}
