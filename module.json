{
  "id": "woodland-generators",
  "title": "Woodland Generators",
  "description": "A random character, NPC, and clearing generator for the Root: The RPG.",
  "version": "#{VERSION}#",
  "library": "false",
  "manifestPlusVersion": "1.2.0",
  "compatibility": {
    "minimum": 10,
    "verified": 10,
    "maximum": 11
  },
  "authors": [
    {
      "name": "alunduil",
      "url": "https://github.com/alunduil",
      "discord": "alunduil"
    }
  ],
  "relationships": {
